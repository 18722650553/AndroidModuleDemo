<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wss.module.main">


    <!--在Module单独开发模式下使用该mainfest-->
    <application
        android:name=".common.MainApplication"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:theme="@style/AdmTheme">

        <activity android:name=".ui.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.page.selector.SelectorActivity" />
        <activity android:name=".ui.page.MultipleItemActivity" />
        <activity android:name=".ui.page.CountViewActivity" />
        <activity android:name=".ui.page.ObserverButtonActivity" />
        <activity android:name=".ui.page.FlowLayoutActivity" />
        <activity android:name=".ui.page.DialogActivity" />
        <activity android:name=".ui.page.RadioGroupActivity" />
        <activity android:name=".ui.page.viscosity.SimpleViscositySlideActivity" />
        <activity android:name=".ui.page.viscosity.ComplexViscositySlideActivity" />
        <activity android:name=".ui.page.navigation.CityListActivity" />
        <activity android:name=".ui.page.CustomViewActivity" />

    </application>
</manifest>